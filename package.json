{
  "name": "Kapla",
  "productName": "Kapla",
  "version": "1.0.0",
  "description": "Instrument, mit dem der Rechtsdienst des AWEL Gesch√§fte erfasst und Fristen kontrolliert",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "lint": "eslint app test *.js",
    "hot-server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
    "package": "cross-env NODE_ENV=production node package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "rebuild-sqlite3-win64": "cd node_modules/sqlite3 && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v48-win32-x64 && node-gyp rebuild --target=1.3.5 --arch=x64 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.3-win32-x64",
    "rebuild-sqlite3-win32": "cd node_modules/sqlite3 && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v48-win32-x32 && node-gyp rebuild --target=1.3.5 --arch=x32 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.3-win32-x32",
    "rebuild-sqlite3-mac": "cd node_modules/sqlite3 && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v48-darwin-x64 && node-gyp rebuild --target=1.3.5 --arch=x64 --target_platform=darwin --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.3-darwin-x64",
    "package2": "set NODE_ENV=DEBUG&& electron-packager . kapla --platform=win32 --arch=x64 --overwrite",
    "buildInstaller": "grunt create-windows-installer"
  },
  "bin": {
    "electron": ".\\node_modules\\.bin\\electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chentsulin/electron-react-boilerplate.git"
  },
  "author": {
    "name": "Alexander Gabriel",
    "email": "alex@gabriel-software.ch",
    "url": "http://gabriel-software.ch"
  },
  "license": "MIT",
  "devDependencies": {
    "asar": "0.12.3",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-webpack-loaders": "0.7.1",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chromedriver": "2.24.1",
    "co-mocha": "1.1.3",
    "concurrently": "2.2.0",
    "cross-env": "2.0.1",
    "css-loader": "0.25.0",
    "del": "2.2.2",
    "electron-packager": "8.0.0",
    "electron-prebuilt": "1.4.0",
    "electron-rebuild": "1.2.1",
    "eslint": "3.5.0",
    "eslint-config-airbnb": "11.1.0",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fbjs-scripts": "0.7.1",
    "grunt-electron-installer": "^2.1.0",
    "imports-loader": "0.6.5",
    "jsdom": "9.5.0",
    "json-loader": "0.5.4",
    "minimist": "1.2.0",
    "mocha": "3.0.2",
    "node-libs-browser": "1.0.0",
    "react-addons-test-utils": "15.3.2",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-logger": "2.6.1",
    "selenium-webdriver": "3.0.0-beta-2",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "dependencies": {
    "css-modules-require-hook": "4.0.3",
    "electron-debug": "1.0.1",
    "exceljs": "0.2.24",
    "file-loader": "^0.9.0",
    "font-awesome": "4.6.3",
    "golden-layout": "1.5.3",
    "jquery": "3.1.0",
    "lodash": "4.16.0",
    "moment": "2.15.0",
    "postcss": "5.2.0",
    "react": "15.3.2",
    "react-bootstrap": "0.30.3",
    "react-bootstrap-daterangepicker": "3.2.2",
    "react-dom": "15.3.2",
    "react-list": "0.8.0",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-bootstrap": "0.23.1",
    "react-router-redux": "4.0.5",
    "react-textarea-autosize": "4.0.5",
    "redux": "3.6.0",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.1.0",
    "sqlite3": "3.1.4",
    "username": "2.2.2"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  }
}
