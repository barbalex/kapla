{
  "name": "Kapla",
  "productName": "Kapla",
  "version": "0.9.0",
  "description": "Instrument, mit dem der Rechtsdienst des AWEL Gesch√§fte erfasst und Fristen kontrolliert",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "lint": "eslint app test *.js",
    "hot-server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
    "package": "cross-env NODE_ENV=production node package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "rebuild-sqlite3-win64": "cd node_modules/sqlite3 && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v48-win32-x64 && node-gyp rebuild --target=1.2.5 --arch=x64 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.2-win32-x64",
    "rebuild-sqlite3-win32": "cd node_modules/sqlite3 && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v48-win32-x32 && node-gyp rebuild --target=1.2.5 --arch=x32 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.2-win32-x32",
    "rebuild-sqlite3-mac": "cd node_modules/sqlite3 && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v48-darwin-x64 && node-gyp rebuild --target=1.2.5 --arch=x64 --target_platform=darwin --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.2-darwin-x64",
    "package2": "set NODE_ENV=DEBUG&& electron-packager . kapla --platform=win32 --arch=x64 --overwrite"
  },
  "bin": {
    "electron": ".\\node_modules\\.bin\\electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chentsulin/electron-react-boilerplate.git"
  },
  "author": {
    "name": "Alexander Gabriel",
    "email": "alex@gabriel-software.ch",
    "url": "http://gabriel-software.ch"
  },
  "license": "MIT",
  "devDependencies": {
    "asar": "0.11.0",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-webpack-loaders": "0.7.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chromedriver": "2.21.2",
    "co-mocha": "1.1.2",
    "concurrently": "2.1.0",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "del": "2.2.1",
    "electron-packager": "7.2.0",
    "electron-prebuilt": "1.2.5",
    "electron-rebuild": "1.1.5",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.9.2",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-react": "5.2.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fbjs-scripts": "0.7.1",
    "imports-loader": "0.6.5",
    "jsdom": "9.4.0",
    "json-loader": "0.5.4",
    "minimist": "1.2.0",
    "mocha": "2.5.3",
    "node-libs-browser": "1.0.0",
    "react-addons-test-utils": "15.1.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-logger": "2.6.1",
    "selenium-webdriver": "2.53.2",
    "sinon": "1.17.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.11.0",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "dependencies": {
    "css-modules-require-hook": "4.0.1",
    "electron-debug": "1.0.1",
    "exceljs": "0.2.9",
    "font-awesome": "4.6.3",
    "golden-layout": "1.5.1",
    "jquery": "2.2.4",
    "lodash": "4.13.1",
    "moment": "2.14.1",
    "postcss": "5.0.21",
    "react": "15.2.0",
    "react-bootstrap": "0.29.5",
    "react-bootstrap-daterangepicker": "3.1.0",
    "react-dom": "15.2.0",
    "react-list": "0.7.20",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "react-router-bootstrap": "0.23.0",
    "react-router-redux": "4.0.5",
    "react-textarea-autosize": "4.0.3",
    "redux": "3.5.2",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.1.0",
    "sqlite3": "3.1.4",
    "username": "2.2.1"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  }
}
